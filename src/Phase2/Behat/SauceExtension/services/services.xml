<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="behat.sauce.listener.job_id.class">Phase2\Behat\SauceExtension\Listener\JobIdListener</parameter>
    </parameters>

    <services>
        <service id="behat.sauce.listener.job_id" class="%behat.sauce.listener.job_id.class%">
            <tag name="behat.event_subscriber" priority="0" />
        </service>
    </services>
</container>
